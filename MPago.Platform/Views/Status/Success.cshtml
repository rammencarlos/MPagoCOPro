@{
}
<main class="main">
    <nav aria-label="breadcrumb" class="breadcrumb-nav border-0 mb-0">
        <div class="container">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item"><a href="#">Pages</a></li>
                <li class="breadcrumb-item active" aria-current="page">404</li>
            </ol>
        </div><!-- End .container -->
    </nav><!-- End .breadcrumb-nav -->

    <div class="error-content text-center">
        <div class="container">
            <h1 class="error-title">Compra realizada con exito</h1><!-- End .error-title -->
            <p>Tu compra se está procesando y se enviará lo mas pronto posible</p>
            <p>collection_id: @ViewData["collection_id"]</p>
            <p>collection_status: @ViewData["collection_status"]</p>
            <p>payment_id: @ViewData["payment_id"]</p>
            <p>payment_type: @ViewData["payment_type"]</p>
            <p>external_reference: @ViewData["external_reference"]</p>
            <a asp-action="index" asp-controller="home" class="btn btn-outline-primary-2 btn-minwidth-lg">
                <span>BACK TO HOMEPAGE</span>
                <i class="icon-long-arrow-right"></i>
            </a>
        </div><!-- End .container -->
    </div><!-- End .error-content text-center -->
</main><!-- End .main -->